fn_revisar_boton_no_safe:
    ;-- Recibe en regA el botón a revisar
    ;-- retorna en regA true o false
    CMP A, (botones)
    JNE _rbns_false
    MOV A, 1
    RET
    _rbns_false:
    MOV A, 0
    RET

fn_revisar_boton:
    ;-- Recibe en regA el botón a revisar
    ;-- retorna en regA true o false
    CMP A, (botones)
    JNE _revisar_boton_false
    _rb_safe_loop:
    MOV A, 0
    CMP A, (botones)
    JNE _rb_safe_loop
    MOV A, 1
    RET
    _revisar_boton_false:
    MOV A, 0
    RET

;--------------------------;
