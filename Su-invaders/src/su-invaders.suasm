meth_revisar_boton_con_especial_safe:
    CMP A, (botones)
    JNE _rbces_false
    MOV B, 50
    MOV A, (milisegundos)
    MOV (var_marca_temporal), A
    _rbces_safe_loop:
    MOV A, (milisegundos)
    CMP A, (var_marca_temporal)
    JGE _rbces_no_wrap
    ADD A, 1000
    _rbces_no_wrap:
    SUB A, (var_marca_temporal)
    CMP A, B
    JLT _rbces_safe_loop
    MOV A, 1
    RET
    _rbces_false:
    MOV A, 0
    RET

